#: .\Lib\site-packages\django\contrib\postgres\apps.py:42
msgid "PostgreSQL extensions"
msgstr "Rozszerzenia PostgreSQL"

#: .\Lib\site-packages\django\contrib\postgres\fields\array.py:21
#: .\Lib\site-packages\django\contrib\postgres\forms\array.py:16
#: .\Lib\site-packages\django\contrib\postgres\forms\array.py:170
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Element %(nth)s w tablicy nie przeszedł walidacji:"

#: .\Lib\site-packages\django\contrib\postgres\fields\array.py:22
msgid "Nested arrays must have the same length."
msgstr "Zagnieżdżone tablice muszą mieć tę samą długość."

#: .\Lib\site-packages\django\contrib\postgres\fields\hstore.py:16
msgid "Map of strings to strings/nulls"
msgstr "Mapowanie ciągów znaków na ciągi znaków/nulle"

#: .\Lib\site-packages\django\contrib\postgres\fields\hstore.py:18
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Wartość „%(key)s” nie jest ciągiem znaków ani nullem."

#: .\Lib\site-packages\django\contrib\postgres\fields\jsonb.py:32
msgid "A JSON object"
msgstr "Obiekt JSON"

#: .\Lib\site-packages\django\contrib\postgres\fields\jsonb.py:34
msgid "Value must be valid JSON."
msgstr "Wartość musi być poprawnym JSON-em."

#: .\Lib\site-packages\django\contrib\postgres\forms\hstore.py:16
msgid "Could not load JSON data."
msgstr "Nie można załadować danych JSON."

#: .\Lib\site-packages\django\contrib\postgres\forms\hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "Wejście musi być słownikiem JSON."

#: .\Lib\site-packages\django\contrib\postgres\forms\jsonb.py:19
#, python-format
msgid "“%(value)s” value must be valid JSON."
msgstr "Wartością „%(value)s” musi być poprawny JSON."

#: .\Lib\site-packages\django\contrib\postgres\forms\ranges.py:37
msgid "Enter two valid values."
msgstr "Podaj dwie poprawne wartości."

#: .\Lib\site-packages\django\contrib\postgres\forms\ranges.py:38
msgid "The start of the range must not exceed the end of the range."
msgstr "Początek zakresu nie może przekroczyć jego końca."

#: .\Lib\site-packages\django\contrib\postgres\forms\ranges.py:86
msgid "Enter two whole numbers."
msgstr "Podaj dwie liczby całkowite."

#: .\Lib\site-packages\django\contrib\postgres\forms\ranges.py:92
msgid "Enter two numbers."
msgstr "Podaj dwie liczby."

#: .\Lib\site-packages\django\contrib\postgres\forms\ranges.py:109
msgid "Enter two valid date/times."
msgstr "Podaj dwie poprawne daty/godziny."

#: .\Lib\site-packages\django\contrib\postgres\forms\ranges.py:115
msgid "Enter two valid dates."
msgstr "Podaj dwie poprawne daty."

#: .\Lib\site-packages\django\contrib\postgres\validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Lista zawiera %(show_value)d element, a nie powinna zawierać więcej niż "
"%(limit_value)d."
msgstr[1] ""
"Lista zawiera %(show_value)d elementów, a nie powinna zawierać więcej niż "
"%(limit_value)d."
msgstr[2] ""
"Lista zawiera %(show_value)d elementów, a nie powinna zawierać więcej niż "
"%(limit_value)d."
msgstr[3] ""
"Lista zawiera %(show_value)d elementów, a nie powinna zawierać więcej niż "
"%(limit_value)d."

#: .\Lib\site-packages\django\contrib\postgres\validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Lista zawiera %(show_value)d element, a powinna zawierać nie mniej niż "
"%(limit_value)d."
msgstr[1] ""
"Lista zawiera %(show_value)d elementów, a powinna zawierać nie mniej niż "
"%(limit_value)d."
msgstr[2] ""
"Lista zawiera %(show_value)d elementów, a powinna zawierać nie mniej niż "
"%(limit_value)d."
msgstr[3] ""
"Lista zawiera %(show_value)d elementów, a powinna zawierać nie mniej niż "
"%(limit_value)d."

#: .\Lib\site-packages\django\contrib\postgres\validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Brak części kluczy: %(keys)s"

#: .\Lib\site-packages\django\contrib\postgres\validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Podano nieznane klucze: %(keys)s"

#: .\Lib\site-packages\django\contrib\postgres\validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr ""
"Upewnij się, że ten zakres jest w całości mniejszy lub równy %(limit_value)s."

#: .\Lib\site-packages\django\contrib\postgres\validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr ""
"Upewnij się, że ten zakres jest w całości większy lub równy %(limit_value)s."
